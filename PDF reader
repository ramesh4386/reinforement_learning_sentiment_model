#!pip install PyPDF2  


import PyPDF2  
  
pdf_file = open('example.pdf', 'rb')  
pdf_reader = PyPDF2.PdfFileReader(pdf_file)  
  
text = ''  
for page in range(pdf_reader.getNumPages()):  
    page_obj = pdf_reader.getPage(page)  
    text += page_obj.extractText()  
  
with open('output.txt', 'w') as file:  
    file.write(text)  
